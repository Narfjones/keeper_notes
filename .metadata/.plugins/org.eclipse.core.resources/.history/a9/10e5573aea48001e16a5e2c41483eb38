spring:
   datasource:
      username: keeper_notes
      password: keeper_notes
      url: jdbc:mysql://localhost:3306/keeper_notes
   
   jpa:
      hibernate:
         ddl-auto: none
      show-sql: true

   sql:
      init:
         mode: never
  
 #When you clone and run this please set sql.init.mode to always on the first run to populate tables and data.
 #Do not change jpa.hibernate.ddl-auto. Keep it as none.      